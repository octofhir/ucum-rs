{
  "name": "ucum-playground",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "vite": "vite",
    "prebuild-wasm": "cd ../ucum-wasm && wasm-pack build --target web --out-name octofhir_ucum_wasm",
    "dev": "vite",
    "build": "sh -c 'pnpm run prebuild-wasm && npx vite build'",
    "preview": "npx vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "octofhir-ucum-wasm": "file:../ucum-wasm"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@tsconfig/svelte": "^5.0.0",
    "svelte": "5.36.12",
    "svelte-check": "4.3.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vite": "7.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.13.1"
}
