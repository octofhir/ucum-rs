{
  "name": "octofhir-ucum-wasm",
  "version": "0.1.3",
  "description": "WebAssembly bindings for the UCUM (Unified Code for Units of Measure) library",
  "repository": {
    "type": "git",
    "url": "https://github.com/octofhir/ucum-rs"
  },
  "author": "OctoFHIR Team <funyloony@gmail.com>",
  "license": "MIT",
  "keywords": [
    "fhir",
    "ucum",
    "units",
    "measurement",
    "healthcare",
    "wasm",
    "webassembly"
  ],
  "files": [
    "pkg/",
    "types/",
    "README.md"
  ],
  "main": "pkg/octofhir_ucum_wasm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "wasm-pack build --target web --out-name octofhir_ucum_wasm",
    "build:web": "wasm-pack build --target web --out-name octofhir_ucum_wasm",
    "build:bundler": "wasm-pack build --target bundler --out-name octofhir_ucum_wasm_bundler",
    "build:node": "wasm-pack build --target nodejs --out-name octofhir_ucum_wasm_node",
    "prepublishOnly": "npm run build && mkdir -p pkg/types && cp -r types/* pkg/types/"
  }
}
